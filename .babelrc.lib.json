{
    "presets": [
        ["@babel/preset-typescript", {
            "isTSX": true,
            "allExtensions": true
        }],
        ["@babel/preset-env", {
            "targets": {
                "ie": "11",
                "edge": "17",
                "firefox": "60",
                "chrome": "67",
                "safari": "11.1"
            },
            "useBuiltIns": "usage",
            "corejs": { "version": 3, "proposals": true }
        }]
    ],
    "plugins": [
        "babel-plugin-transform-class-properties"
    ]
}
